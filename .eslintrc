{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
      },
      "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended",
        "prettier",
        "plugin:tailwindcss/recommended"
      ],  
      "plugins": ["@typescript-eslint", "import"],
      "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
  }
}